<h1>Specialties</h1>
<p><a href='/specialties/new'>Add a New Specialty<a/></p>
<p><a href='/'>Back to Home<a/></p>
<table>
  <tr>
    <th>Specialty</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  @foreach(var specialty in Model)
  {
  <tr>
    <td><a href="/specialties/@specialty.specialtyId">@specialty.specialtyName</a></td>
    <td><a href="/specialties/@specialty.specialtyId/edit">Edit</a></td>
    <td><a href="/specialties/@specialty.specialtyId/delete">Delete</a></td>
  </tr>
  }
</table>
