<h1>Stylists</h1>
<p><a href='/stylists/new'>Add a New Stylist<a/></p>
<p><a href='/'>Back to Home<a/></p>
<table>
  <tr>
    <th>Name</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  @foreach(var stylist in Model)
  {
  <tr>
    <td><a href="/stylists/@stylist.Id">@stylist.Name</a></td>
    <td><a href="/stylists/@stylist.Id/edit">Edit</a></td>
    <td><a href="/stylists/@stylist.Id/delete">Delete</a></td>
  </tr>
  }
</table>
